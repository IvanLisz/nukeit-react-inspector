{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "dom.iterable",
      "dom",
      "esnext"
    ],
    "types": ["vite/client", "node"],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "noEmit": true,
    "jsx": "react"
  },
  "include": ["./src"]
}
